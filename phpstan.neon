includes:
    - vendor/szepeviktor/phpstan-wordpress/extension.neon
parameters:
    level: 6
    tmpDir: var/cache/phpstan
    paths:
        - src
        - dwt-localfonts.php
        - uninstall.php
    scanFiles:
        - vendor/php-stubs/wordpress-stubs/wordpress-stubs.php
        - vendor/php-stubs/wp-cli-stubs/wp-cli-stubs.php
    bootstrapFiles:
      - vendor/autoload.php
    excludePaths:
        - vendor
        - tests
        - node_modules (?)
        - strauss